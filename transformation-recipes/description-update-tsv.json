{
	"key": "description-update-tsv",
	"descriptions": "Update the acceptability and case significance of existing descriptions in batch. The 'typeId' and 'term' fields are just for reference, they are ignored by this function.",
	"title": "Description Changes",
	"source": "tsv",
	"component": "DESCRIPTION",
	"changeType": "UPDATE",
	"fieldMap": {
		"descriptionId": "$tsv.0",
		"preferredTerm": "$tsv.1",
		"term": "$tsv.2",
		"caseSignificanceId": "$tsv.3|map-constant",
		"typeId": "$tsv.4|map-constant",
		"acceptability": [
			{
				"langRefset1": "$tsv.5|map-constant",
				"acceptability1": "$tsv.6|map-constant"
			},
			{
				"langRefset2": "$tsv.7|map-constant",
				"acceptability2": "$tsv.8|map-constant"
			},
			{
				"langRefset3": "$tsv.9|map-constant",
				"acceptability3": "$tsv.10|map-constant"
			}
		],
		"notes": "$tsv.11"
	}
}
