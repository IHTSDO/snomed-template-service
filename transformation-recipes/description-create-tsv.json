{
	"key": "description-create-tsv",
	"title": "Description Additions",
	"description": "Add descriptions to existing concepts in batch. Acceptability can be given for up to three language reference sets. Module will be set automatically.",
	"source": "tsv",
	"component": "DESCRIPTION",
	"changeType": "CREATE",
	"fieldMap": {
		"conceptId": "$tsv.0",
		"termRef": "$tsv.1",
		"preferredTerm": "$tsv.2",
		"term": "$tsv.3",
		"lang": "$tsv.4",
		"caseSignificanceId": "$tsv.5|map-constant",
		"typeId": "$tsv.6|map-constant",
		"acceptability": [
			{
				"langRefset1": "$tsv.7|map-constant",
				"acceptability1": "$tsv.8|map-constant"
			},
			{
				"langRefset2": "$tsv.9|map-constant",
				"acceptability2": "$tsv.10|map-constant"
			},
			{
				"langRefset3": "$tsv.11|map-constant",
				"acceptability3": "$tsv.12|map-constant"
			}
		]
	}
}
